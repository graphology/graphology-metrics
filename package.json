{
  "name": "graphology-metrics",
  "version": "1.14.2",
  "description": "Miscellaneous graph metrics for graphology.",
  "main": "index.js",
  "files": [
    "*.js",
    "*.d.ts",
    "centrality",
    "layout-quality"
  ],
  "scripts": {
    "lint": "eslint **/*.js",
    "prepublish": "npm run lint && npm test",
    "test": "mocha --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphology/graphology-metrics.git"
  },
  "keywords": [
    "betweenness",
    "centrality",
    "density",
    "diameter",
    "eccentricity",
    "extent",
    "graph",
    "graphology",
    "metrics",
    "modularity"
  ],
  "contributors": [
    {
      "name": "Guillaume Plique",
      "url": "http://github.com/Yomguithereal"
    },
    {
      "name": "Tanguy Lucci",
      "url": "https://github.com/luccitan"
    },
    {
      "name": "Pauline Breteau",
      "url": "https://github.com/paubre"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphology/graphology-metrics/issues"
  },
  "homepage": "https://github.com/graphology/graphology-metrics#readme",
  "devDependencies": {
    "@yomguithereal/eslint-config": "^4.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "graphology": "^0.19.1",
    "graphology-generators": "^0.10.1",
    "graphology-layout": "^0.2.0",
    "graphology-operators": "^1.3.0",
    "graphology-types": "^0.19.0",
    "mocha": "^8.2.1"
  },
  "peerDependencies": {
    "graphology-types": ">=0.19.0"
  },
  "eslintConfig": {
    "extends": "@yomguithereal/eslint-config",
    "rules": {
      "space-before-function-paren": [
        2,
        "never"
      ]
    },
    "globals": {
      "Set": true,
      "Float64Array": true
    }
  },
  "dependencies": {
    "graphology-shortest-path": "^1.4.0",
    "graphology-utils": "^1.8.0",
    "lodash": "^4.17.20",
    "mnemonist": "^0.38.0"
  }
}
